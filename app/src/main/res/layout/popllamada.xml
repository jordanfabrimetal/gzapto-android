                    app:layout_constraintTop_toBottomOf="@+id/view33" />

                <EditText
                    android:id="@+id/etCodCli"
                    android:layout_width="209dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:hint="@string/no_hay_codigo"
                    android:inputType="text"
                    android:minHeight="48dp"
                    android:textSize="18sp"
                    android:autofillHints="false"
                    android:visibility="visible"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tvCliOb" />

                <EditText
                    android:id="@+id/etObservacionIni"
                    android:layout_width="290dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:ems="10"
                    android:gravity="start|top"
                    android:autofillHints="true"
                    android:hint="@string/escribe_las_observaciones_iniciales"
                    android:inputType="textMultiLine"
                    android:lines="4"
                    android:overScrollMode="always"
                    android:scrollbarStyle="insideInset"
                    android:scrollbars="vertical"
                    android:visibility="gone"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/etCodCli"
                    tools:inputType="textMultiLine" />


            </androidx.constraintlayout.widget.ConstraintLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>

        <com.google.android.material.appbar.AppBarLayout
            android:id="@+id/appBarLayout"
