                    android:onClick="buscarEquipo"
                    android:contentDescription="@string/continuar"
                    android:visibility="gone"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/codigo"
