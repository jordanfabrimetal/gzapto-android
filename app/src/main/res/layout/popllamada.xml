                    android:id="@+id/gifImageView8"
                    android:layout_width="110dp"
                    android:layout_height="109dp"
                    android:layout_marginStart="-20dp"
                    android:layout_marginTop="-20dp"
                    android:src="@drawable/ultrocket"
